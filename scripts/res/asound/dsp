pcm.!default {
        type copy
        slave.pcm "dsp"
}


pcm.dsp {
        type plug
        slave {
                format FLOAT
                rate unchanged
                pcm {
                        type ladspa
#                       channels 4
                        path "/etc/lib/ladspa"
                        playback_plugins [{
                                label "ladspa_dsp"
                        }]
                        slave.pcm {
                                type plug
                                slave {
                                        pcm "sysdefault:0"
                                        rate unchanged
                                        channels unchanged
                                }
                        }
                }
        }
}
